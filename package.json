{
  "name": "ts-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.2.1",
    "apollo-cache-inmemory": "^1.1.7",
    "apollo-client": "^2.2.3",
    "apollo-codegen": "^0.18.3",
    "apollo-link-schema": "^1.0.3",
    "graphql": "^0.13.0",
    "graphql-tag": "^2.7.3",
    "graphql-tools": "^2.20.2",
    "normalize.css": "^8.0.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0",
    "styled-components": "^3.1.6"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "format": "tslint --fix 'src/**/*.+(ts|tsx)'",
    "schema": "apollo-codegen introspect-schema ./src/graphql/schema.graphql --output ./src/graphql/schema.json",
    "types": "apollo-codegen generate ./src/**/queries.ts --addTypename --schema ./src/graphql/schema.json --target typescript --output ./src/graphql/__generated__/types.ts"
  },
  "devDependencies": {
    "@types/graphql": "^0.12.3",
    "@types/jest": "^22.1.2",
    "@types/lodash": "^4.14.102",
    "@types/node": "^9.4.5",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-router": "^4.0.21",
    "@types/react-router-dom": "^4.2.3",
    "@types/webpack-env": "^1.13.5",
    "prettier": "^1.10.2",
    "prettier-tslint": "^0.3.0",
    "typescript": "^2.7.1"
  }
}
